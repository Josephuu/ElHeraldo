<!-- Podcast -->
<div class="container-fluid">

    <div class="row justify-content-center border " style="background-color: #212529;">
        <h1
            class=" videos-h1-color text-white mt-sm-5 pt-sm-3 mt-md-5 pt-md-3 mt-lg-0 pt-lg-0 mt-xl-0 pt-xl-0 p-lg-1 p-xl-1">
            PODCAST</h1>
    </div>
    <div class="container-fluid p-0">
        {{#each podcastDoc}}
        <div class="row" style="background-color: #212529;">
            <div id="videos" class="col-lg" style="background-color: #212529;">
                <div class="col-lg d-flex flex-column">
                    <iframe src="{{spotifyFrame}}" width="100%" height="158" frameborder="0" allowtransparency="true"
                        allow="encrypted-media"></iframe>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>
<!-- /Podcast -->