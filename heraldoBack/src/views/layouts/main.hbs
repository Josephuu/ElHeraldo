<!-- Sidebar -->
<script>
    function openNav() {
        document.getElementById("mySidenav").style.width = "250px";
        $('.overlay').addClass('active');
    }

    function closeNav() {
        $('#BotonCerrar, .overlay').on('click', function () {
            $('.overlay').removeClass('active');
            document.getElementById("mySidenav").style.width = "0";
        });
    }

</script>
